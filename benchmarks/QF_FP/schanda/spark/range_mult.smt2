(set-info :smt-lib-version 2.6)
;;; Processed by pysmt to remove constant-real bitvector literals
(set-logic QF_FP)
(set-info :source |SPARK inspired floating point problems by Florian Schanda|)
(set-info :category "crafted")
(set-info :status unsat)
(define-fun isFinite ((f Float32)) Bool (or (fp.isNormal f) (fp.isSubnormal f) (fp.isZero f)))
(define-fun f2 () Float32 ((_ to_fp 8 24) #x40000000))
(define-fun f5 () Float32 ((_ to_fp 8 24) #x40a00000))
(define-fun f10 () Float32 ((_ to_fp 8 24) #x41200000))
(declare-fun x () Float32)
(assert (fp.leq f5 x f10))
(define-fun y () Float32 (fp.sub RNE (fp.mul RNE x f2) f5))
(assert (not (and (fp.geq y x) (isFinite y))))
(check-sat)
(exit)
