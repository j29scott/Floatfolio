(set-info :smt-lib-version 2.6)
;;; Processed by pysmt to remove constant-real bitvector literals
(set-logic QF_FP)
(set-info :source |SPARK inspired floating point problems by Florian Schanda|)
(set-info :category "crafted")
(set-info :status sat)
(define-fun is_finite ((f Float32)) Bool (or (fp.isNormal f) (fp.isSubnormal f) (fp.isZero f)))
(define-fun f2 () Float32 ((_ to_fp 8 24) #x40000000))
(declare-fun a () Float32)
(declare-fun b () Float32)
(assert (is_finite (fp.add RNE a b)))
(assert (fp.lt a b))
(define-fun mid () Float32 (fp.div RNE (fp.add RNE a b) f2))
(assert (not (and (fp.lt a mid) (fp.lt mid b))))
(check-sat)
(exit)
