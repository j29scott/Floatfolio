(set-logic QF_FP)
(declare-const x0 (_ FloatingPoint 8 24))
(declare-const x1 (_ FloatingPoint 8 24))
(declare-const x2 (_ FloatingPoint 8 24))
(declare-const x3 (_ FloatingPoint 8 24))
(assert (fp.eq (fp.div RNE x1 x0) (fp.add RNE x1 x0)))
(assert (fp.lt (fp.div RNE x2 x0) (fp.mul RNE x2 x1)))
(assert (fp.lt (fp.div RNE x1 x2) (fp.mul RNE x1 x0)))
(assert (fp.geq (fp.add RNE x2 x1) (fp.sub RNE x0 x1)))
(assert (fp.eq (fp.add RNE x0 x2) (fp.neg x0)))
(assert (fp.gt (fp.neg x3) (fp.add RNE x3 x3)))
(check-sat)
