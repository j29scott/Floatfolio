(set-logic QF_FP)
(declare-const x0 (_ FloatingPoint 8 24))
(declare-const x1 (_ FloatingPoint 8 24))
(declare-const x2 (_ FloatingPoint 8 24))
(declare-const x3 (_ FloatingPoint 8 24))
(assert (fp.eq (fp.add RNE x0 x3) (fp.div RNE x2 x2)))
(assert (fp.geq (fp.neg x0) (fp.neg x3)))
(assert (fp.leq (fp.neg x0) (fp.div RNE x0 x3)))
(assert (fp.eq (fp.neg x1) (fp.add RNE x1 x3)))
(assert (fp.gt (fp.neg x2) (fp.mul RNE x3 x1)))
(assert (fp.gt (fp.neg x1) (fp.add RNE x0 x2)))
(assert (fp.eq (fp.neg x0) (fp.neg x1)))
(check-sat)
