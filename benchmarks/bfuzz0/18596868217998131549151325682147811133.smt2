(set-logic QF_FP)
(declare-const x0 (_ FloatingPoint 8 24))
(declare-const x1 (_ FloatingPoint 8 24))
(declare-const x2 (_ FloatingPoint 8 24))
(declare-const x3 (_ FloatingPoint 8 24))
(assert (fp.leq (fp.div RNE x2 x1) (fp.neg x2)))
(assert (fp.eq (fp.div RNE x2 x0) (fp.mul RNE x1 x1)))
(assert (fp.lt (fp.add RNE x2 x1) (fp.mul RNE x0 x2)))
(assert (fp.eq (fp.div RNE x3 x3) (fp.mul RNE x1 x2)))
(assert (fp.gt (fp.mul RNE x1 x3) (fp.neg x3)))
(assert (fp.eq (fp.add RNE x2 x3) (fp.sub RNE x1 x1)))
(assert (fp.leq (fp.div RNE x3 x0) (fp.mul RNE x0 x1)))
(check-sat)
