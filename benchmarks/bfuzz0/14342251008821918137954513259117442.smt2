(set-logic QF_FP)
(declare-const x0 (_ FloatingPoint 8 24))
(declare-const x1 (_ FloatingPoint 8 24))
(declare-const x2 (_ FloatingPoint 8 24))
(assert (fp.lt (fp.add RNE x0 x1) (fp.neg x2)))
(assert (fp.geq (fp.add RNE x2 x1) (fp.div RNE x1 x0)))
(assert (fp.gt (fp.sub RNE x1 x0) (fp.div RNE x0 x2)))
(assert (fp.eq (fp.div RNE x2 x2) (fp.neg x1)))
(assert (fp.leq (fp.sub RNE x0 x0) (fp.sub RNE x0 x1)))
(assert (fp.leq (fp.div RNE x0 x2) (fp.div RNE x2 x2)))
(check-sat)
