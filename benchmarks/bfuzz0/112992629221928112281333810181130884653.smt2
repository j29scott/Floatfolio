(declare-const x3 (_ FloatingPoint 8 24))
(declare-const x4 (_ FloatingPoint 8 24))
(assert (fp.gt (fp.neg (fp.sub RNE x4 x2)) (fp.neg (fp.add RNE x0 x4))))
