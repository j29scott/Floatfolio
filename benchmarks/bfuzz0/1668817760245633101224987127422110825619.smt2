(set-logic QF_FP)
(declare-const x0 (_ FloatingPoint 8 24))
(declare-const x1 (_ FloatingPoint 8 24))
(declare-const x2 (_ FloatingPoint 8 24))
(declare-const x3 (_ FloatingPoint 8 24))
(assert (fp.lt (fp.add RNE x3 x2) (fp.mul RNE x1 x1)))
(assert (fp.lt (fp.sub RNE x1 x0) (fp.neg x1)))
(assert (fp.eq (fp.neg x2) (fp.sub RNE x2 x1)))
(assert (fp.geq (fp.mul RNE x3 x0) (fp.neg x3)))
(assert (fp.eq (fp.neg x0) (fp.mul RNE x3 x3)))
(assert (fp.lt (fp.sub RNE x1 x2) (fp.sub RNE x0 x3)))
(assert (fp.leq (fp.div RNE x3 x3) (fp.neg x3)))
(assert (fp.lt (fp.add RNE x2 x0) (fp.sub RNE x0 x3)))
(assert (fp.leq (fp.sub RNE x3 x3) (fp.neg x2)))
(check-sat)
