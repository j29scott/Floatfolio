(set-logic QF_FP)
(declare-const x0 (_ FloatingPoint 8 24))
(declare-const x1 (_ FloatingPoint 8 24))
(declare-const x2 (_ FloatingPoint 8 24))
(assert (fp.leq (fp.neg x0) (fp.neg x1)))
(assert (fp.lt (fp.div RNE x2 x2) (fp.mul RNE x2 x2)))
(assert (fp.gt (fp.mul RNE x0 x1) (fp.div RNE x1 x0)))
(assert (fp.leq (fp.neg x2) (fp.neg x1)))
(assert (fp.eq (fp.add RNE x2 x2) (fp.add RNE x0 x1)))
(assert (fp.eq (fp.mul RNE x0 x2) (fp.sub RNE x2 x1)))
(assert (fp.eq (fp.neg x2) (fp.mul RNE x2 x0)))
(assert (fp.lt (fp.add RNE x2 x1) (fp.sub RNE x0 x0)))
(assert (fp.geq (fp.mul RNE x2 x2) (fp.add RNE x2 x1)))
(check-sat)
