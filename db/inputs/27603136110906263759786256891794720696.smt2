(set-logic QF_FP)
(declare-const x0 (_ FloatingPoint 8 24))
(declare-const x1 (_ FloatingPoint 8 24))
(declare-const x2 (_ FloatingPoint 8 24))
(declare-const x3 (_ FloatingPoint 8 24))
(assert (fp.geq (fp.neg x1) (fp.mul RNE x0 x3)))
(assert (fp.geq (fp.mul RNE x3 x1) (fp.neg x1)))
(assert (fp.leq (fp.div RNE x0 x3) (fp.mul RNE x2 x0)))
(assert (fp.eq (fp.sub RNE x2 x3) (fp.neg x1)))
(assert (fp.eq (fp.div RNE x0 x2) (fp.sub RNE x2 x3)))
(assert (fp.geq (fp.neg x0) (fp.add RNE x2 x0)))
(assert (fp.geq (fp.neg x0) (fp.mul RNE x0 x2)))
(check-sat)
