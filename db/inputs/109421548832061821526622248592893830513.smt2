(set-logic QF_FP)
(declare-const x0 (_ FloatingPoint 8 24))
(declare-const x1 (_ FloatingPoint 8 24))
(assert (fp.geq (fp.mul RNE x1 x0) (fp.sub RNE x0 x0)))
(assert (fp.eq (fp.sub RNE x0 x1) (fp.add RNE x0 x0)))
(assert (fp.leq (fp.sub RNE x1 x1) (fp.sub RNE x0 x0)))
(assert (fp.geq (fp.div RNE x1 x0) (fp.add RNE x1 x0)))
(assert (fp.leq (fp.sub RNE x1 x1) (fp.div RNE x1 x0)))
(check-sat)
